                     </children>
                  </Pane>
                  <Pane fx:id="centro" prefHeight="648.0" prefWidth="727.0">
                     <children>
                        <Button fx:id="rollDiceButton" alignment="TOP_RIGHT" layoutX="321.0" layoutY="539.0" mnemonicParsing="false" onAction="#rollDiceClick" styleClass="style.css" text="Roll Dice" />
                        <TextField fx:id="currentPoints" layoutX="308.0" layoutY="486.0" prefHeight="25.0" prefWidth="117.0" />
                        <TextArea fx:id="visorDados" layoutX="88.0" layoutY="93.0" prefHeight="200.0" prefWidth="516.0" />
                     </children>
                  </Pane>
               </children></VBox>
            <VBox prefHeight="720.0" prefWidth="450.0">
               <children>
                  <ImageView fx:id="derecha" fitHeight="695.0" fitWidth="276.0" pickOnBounds="true" preserveRatio="true" />
               </children></VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
