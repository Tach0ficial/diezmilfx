<AnchorPane stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diezMil.GameController">
   <children>
      <HBox prefHeight="720.0" prefWidth="1280.0">
         <children>
            <VBox fx:id="izquierda" prefHeight="720.0" prefWidth="450.0">
               <children>
                  <Pane fx:id="izquierda" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Text fx:id="PlayerName" layoutX="106.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Text>
                        <Text fx:id="PlayerPoints" layoutX="106.0" layoutY="168.0" strokeType="OUTSIDE" strokeWidth="0.0" text="5000">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Text>
                     </children>
                  </Pane>
                  <TableView fx:id="tableRounds" fixedCellSize="2.0" prefHeight="523.0" prefWidth="277.0">
                    <columns>
                      <TableColumn fx:id="tableRoundsColunm" maxWidth="85.0" minWidth="85.0" prefWidth="85.0" resizable="false" styleClass="style.css" />
                      <TableColumn fx:id="tablePointsColunm" maxWidth="201.0" minWidth="191.0" prefWidth="191.0" resizable="false" styleClass="style.css" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox fx:id="centro" prefHeight="720.0" prefWidth="900.0">
               <children>
                  <Pane fx:id="centro" prefHeight="85.0" prefWidth="727.0">
                     <children>
                        <Button alignment="TOP_RIGHT" layoutX="510.0" layoutY="29.0" mnemonicParsing="false" text="tabla" />
